   "source": [
    "import math\n",
    "import random\n",
    "s = 100000\n",
    "hit = 0.0\n",
    "d = 0.0\n",
    "for i in range(1, s + 1):\n",
    "    x = random.uniform(0.0,1.0)\n",
    "    y = random.uniform(0.0,1.0)\n",
    "    d = math.sqrt(x ** 2 + y ** 2)\n",
    "    if d <= 1.0:\n",
    "        hit = hit + 1\n",
    "P = 4 * (hit / s)\n",
    "\n",
    "print(P)"
   ]
