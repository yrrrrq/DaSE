   "source": [
    "def bubble_sort(array):\n",
    "    for i in range(1, len(array)):\n",
    "        for j in range(0, len(array) - i):\n",
    "            if array[j] > array[j+1] :\n",
    "                array[j], array[j+1] = array[j+1], array[j]\n",
    "    return array\n",
    "\n",
    "array = [43, 36, 25, 4, 7, 98, 21]\n",
    "print(bubble_sort(array))"
   ]
